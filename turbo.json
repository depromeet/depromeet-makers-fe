{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"]
    },
    "web#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "android": {
      "dependsOn": ["^android"]
    },
    "ios": {
      "dependsOn": ["^ios"]
    },
    "ios:build:dev": {
      "dependsOn": ["^ios:build:dev"]
    }
  }
}
